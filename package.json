{
  "name": "astro-activity-calendar",
  "version": "1.0.0",
  "description": "A calendar component for displaying GitHub activity",
  "author": {
    "name": "Incognity",
    "email": "contact@incognity.dev",
    "url": "https://incognity.dev"
  },
  "type": "module",
  "exports": {
    ".": "./index.ts",
    "./types": "./index.types.ts",
    "./data": "./index.data.ts"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zincognity/astro-activity-calendar.git"
  },
  "files": [
    "src",
    "index.ts",
    "index.types.ts",
    "index.data.ts"
  ],
  "keywords": [
    "astro",
    "calendar",
    "github",
    "activity"
  ],
  "scripts": {
    "build": "astro build",
    "lint": "eslint . --ext .astro,.js,.jsx,.ts,.tsx,.mdx",
    "lint:fix": "eslint . --ext .astro,.js,.jsx,.ts,.tsx,.mdx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check",
    "type:check": "astro check"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,astro,jsx,tsx,mdx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "tailwindcss": "^4.1.12",
    "@tailwindcss/vite": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "9.34.0",
    "@typescript-eslint/parser": "8.41.0",
    "astro": "^5.13.4",
    "eslint": "9.34.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-astro": "1.3.1",
    "eslint-scope": "8.4.0",
    "lint-staged": "16.1.5",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-astro-organize-imports": "0.4.11",
    "prettier-plugin-organize-imports": "4.2.0",
    "prettier-plugin-tailwindcss": "0.6.14",
    "typescript": "5.9.2",
    "typescript-eslint": "8.41.0"
  }
}
